

/* Event message ID's */
#define EA_INIT_INF_EID       1  /* start up message "informational" */
#define EA_NOOP_INF_EID       2  /* processed command "informational" */
#define EA_RESET_INF_EID      3
#define EA_PROCCESS_INF_EID   4
#define EA_MID_ERR_EID        5  /* invalid command packet "error" */
#define EA_CC1_ERR_EID        6
#define EA_LEN_ERR_EID        7
#define EA_PIPE_ERR_EID       8
#define EA_EVT_COUNT          8  /* count of event message ID's */
